package Main1 {
import java.util.Scanner;

abstract class Employee{
abstract void work();
}
interface Eatable{
public void eat();
}
class Chef implements Eatable{
String name ;
Chef(String name){
this.name =name;
}
public void eat(){
System.out.print("Chef"+name+"is tasting the food.");
}
}
class Waiter implements Eatable{
String name ;
Waiter(String name){
this.name =name;
}
public void eat(){
System.out.print("Waiter"+name+"is having a quick meal break.");
}
}
public class Main{
public static void main (String[]args){

Scanner input=new Scanner(System.in);
String name=input.nextline();
Chef objCh =new Chef(name); 
Waiter objW =new Waiter(name);
objCh.eat();
objW.eat();

}
}
}
}
